{
  "name": "github-mcp-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "cd client && vite",
    "dev:full": "concurrently -n v,s \"pnpm dev\" \"pnpm dev:server\"",
    "build": "cd client && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "cd client && vite preview",
    "dev:server": "tsx server/index.ts",
    "setup-db": "node scripts/setup-db.js",
    "validate": "node scripts/validate-startup.js",
    "start": "pnpm validate && pnpm setup-db && pnpm dev:full",
    "quick-start": "pnpm dev:full"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.59.0",
    "@trpc/client": "^11.0.0-rc.600",
    "@trpc/react-query": "^11.0.0-rc.600",
    "@trpc/server": "^11.0.0-rc.600",
    "@types/cors": "^2.8.19",
    "clsx": "^2.1.1",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.36.0",
    "lucide-react": "^0.454.0",
    "openai": "^4.73.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^1.5.0",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^2.5.4",
    "wouter": "^3.9.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "terser": "^5.46.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
